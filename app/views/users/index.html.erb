<p id="notice"><%= notice %></p>
<%= link_to 'New User', new_user_path %>

<h1>Users</h1>
<%= will_paginate %>
<table>
  <thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Age</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
  <% end %>
  </tbody>
</table>
<%= will_paginate %>
<br>


